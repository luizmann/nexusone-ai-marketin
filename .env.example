# NexusOne AI Environment Configuration
# Copy this file to .env.local and fill in your API keys

# ===============================================
# REQUIRED - Core Infrastructure
# ===============================================

# Supabase Configuration (Required)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# OpenAI Configuration (Required for AI features)
VITE_OPENAI_API_KEY=sk-proj-your-openai-api-key
VITE_OPENAI_ASSISTANT_ID=asst_your-assistant-id
OPENAI_API_KEY=sk-proj-your-openai-api-key

# ===============================================
# OPTIONAL - AI Services (Enable specific features)
# ===============================================

# Video Generation APIs
VITE_LUMA_API_KEY=luma-your-luma-api-key
VITE_RUNWAY_API_KEY=rw-your-runway-api-key
LUMA_API_KEY=luma-your-luma-api-key
RUNWAY_API_KEY=rw-your-runway-api-key

# Audio Generation
VITE_ELEVEN_LABS_API_KEY=sk_your-eleven-labs-key
ELEVEN_LABS_API_KEY=sk_your-eleven-labs-key

# Image Generation
VITE_REPLICATE_API_TOKEN=r8_your-replicate-token
REPLICATE_API_TOKEN=r8_your-replicate-token

# Image Sources
VITE_UNSPLASH_ACCESS_KEY=your-unsplash-access-key
UNSPLASH_ACCESS_KEY=your-unsplash-access-key

# ===============================================
# OPTIONAL - Marketing APIs
# ===============================================

# Facebook Marketing
VITE_FACEBOOK_ACCESS_TOKEN=your-facebook-access-token
FACEBOOK_ACCESS_TOKEN=your-facebook-access-token
VITE_FACEBOOK_APP_ID=your-facebook-app-id
FACEBOOK_APP_ID=your-facebook-app-id

# WhatsApp Business
VITE_GUPSHUP_API_KEY=your-gupshup-api-key
GUPSHUP_API_KEY=your-gupshup-api-key
VITE_WHATSAPP_API_TOKEN=your-whatsapp-token
WHATSAPP_API_TOKEN=your-whatsapp-token

# Google Services
VITE_GOOGLE_ADS_API_KEY=your-google-ads-key
GOOGLE_ADS_API_KEY=your-google-ads-key

# ===============================================
# OPTIONAL - E-commerce APIs
# ===============================================

# Dropshipping
VITE_CJ_DROPSHIPPING_API_KEY=your-cj-dropshipping-key
CJ_DROPSHIPPING_API_KEY=your-cj-dropshipping-key

# Payment Processing
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-key
STRIPE_SECRET_KEY=sk_test_your-stripe-secret
VITE_PAYPAL_CLIENT_ID=your-paypal-client-id
PAYPAL_CLIENT_SECRET=your-paypal-secret

# ===============================================
# OPTIONAL - Development & Analytics
# ===============================================

# Environment
NODE_ENV=development
VITE_APP_ENV=development

# Analytics
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Error Tracking
VITE_SENTRY_DSN=your-sentry-dsn
SENTRY_DSN=your-sentry-dsn

# ===============================================
# OPTIONAL - Security & Encryption
# ===============================================

# JWT Secrets
JWT_SECRET=your-jwt-secret-key-here
NEXTAUTH_SECRET=your-nextauth-secret-key

# Encryption
ENCRYPTION_KEY=your-32-character-encryption-key

# ===============================================
# OPTIONAL - External Integrations
# ===============================================

# Email Services
SENDGRID_API_KEY=SG.your-sendgrid-key
RESEND_API_KEY=re_your-resend-key

# Storage
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your-s3-bucket

# ===============================================
# API Configuration
# ===============================================

# Rate Limiting
API_RATE_LIMIT_REQUESTS=100
API_RATE_LIMIT_WINDOW=3600

# CORS
ALLOWED_ORIGINS=http://localhost:3000,https://your-domain.com

# ===============================================
# Feature Flags
# ===============================================

# Enable/Disable Features
VITE_ENABLE_VIDEO_GENERATION=true
VITE_ENABLE_DROPSHIPPING=true
VITE_ENABLE_WHATSAPP=true
VITE_ENABLE_FACEBOOK_ADS=true
VITE_ENABLE_AI_AGENTS=true

# ===============================================
# Pricing Configuration
# ===============================================

# Credit Costs (per operation)
VITE_CREDIT_COST_MAGIC_PAGE=10
VITE_CREDIT_COST_VIDEO=25
VITE_CREDIT_COST_CAMPAIGN=50
VITE_CREDIT_COST_AI_AGENT=20

# Plan Limits
VITE_FREE_PLAN_CREDITS=50
VITE_PRO_PLAN_CREDITS=500
VITE_PREMIUM_PLAN_CREDITS=2000

# ===============================================
# Multi-language Configuration
# ===============================================

# Default Language
VITE_DEFAULT_LANGUAGE=en

# Supported Languages
VITE_SUPPORTED_LANGUAGES=en,pt,es,he,ar

# ===============================================
# Deployment Configuration
# ===============================================

# App URLs
VITE_APP_URL=http://localhost:3000
VITE_API_URL=http://localhost:54321/functions/v1

# Production URLs (update for production)
# VITE_APP_URL=https://nexusone.ai
# VITE_API_URL=https://your-project.supabase.co/functions/v1

# ===============================================
# NOTES:
# ===============================================

# 1. Variables starting with VITE_ are accessible in the frontend
# 2. Variables without VITE_ are only available in backend/edge functions
# 3. Never commit real API keys to version control
# 4. Use different keys for development and production
# 5. Some features will be disabled if their API keys are not provided
# 6. Check the documentation for API key setup instructions

